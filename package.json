{
  "name": "life-tracker",
  "version": "1.0.0",
  "description": "A comprehensive personal life tracking application designed to help you monitor, analyze, and improve various aspects of your daily life",
  "main": "server/index.js",
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/life-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/life-tracker/issues"
  },
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "cross-env BROWSER=none react-scripts start",
    "server:dev": "nodemon --exec \"ts-node --project tsconfig.server.json server/index.ts\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "react-scripts build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "react-scripts test --coverage --watchAll=false",
    "test:server": "jest server/ --coverage",
    "test:e2e": "cypress run",
    "test:watch": "react-scripts test",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint src/ --ext .ts,.tsx",
    "lint:server": "eslint server/ --ext .ts",
    "lint:fix": "npm run lint:client -- --fix && npm run lint:server -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.server.json --noEmit",
    "migrate": "knex migrate:latest --knexfile server/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile server/config/knexfile.js",
    "seed": "knex seed:run --knexfile server/config/knexfile.js",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "db:maintenance": "ts-node scripts/db-maintenance.ts",
    "db:backup": "ts-node scripts/db-maintenance.ts --backup",
    "db:optimize": "ts-node scripts/db-maintenance.ts --vacuum --analyze",
    "clean": "rm -rf build/ dist/ coverage/"
  },
  "keywords": [
    "life-tracker",
    "habits",
    "goals",
    "analytics",
    "health",
    "wellness",
    "productivity",
    "self-improvement",
    "tracking",
    "dashboard"
  ],
  "author": "Life Tracker Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "active-win": "^8.2.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lucide-react": "^0.539.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "msw": "^2.0.11",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "react-scripts": "5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.9.5"
  },
  "proxy": "http://localhost:8000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "server/**/*.{ts,js}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
